{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "include": ["."],
        "exclude": ["node_modules"]
    },
	"plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "opts": {
        "destination": "./docs/",
        "recurse": true,
		"template": "node_modules/minami"
    }
}
